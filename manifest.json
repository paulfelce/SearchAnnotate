{
  "manifest_version": 3,
  "name": "Search Result Annotator",
  "version": "1.1",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}